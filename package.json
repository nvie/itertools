{
    "name": "itertools",
    "version": "1.7.1",
    "description": "A JavaScript port of Python's awesome itertools standard library",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nvie/itertools.js.git"
    },
    "author": "Vincent Driessen",
    "homepage": "https://github.com/nvie/itertools.js#readme",
    "bugs": {
        "url": "https://github.com/nvie/itertools.js/issues"
    },
    "main": "./dist/index.js",
    "scripts": {
        "actually-prepublish": "echo \"\\n\\nPlease run \\`bin/publish.sh <options>\\` instead.\\n\\n\" && exit 2",
        "build": "./bin/build.sh",
        "check": "npm run check:eslint && npm run check:prettier && npm run check:import-sort",
        "check:eslint": "npx eslint --report-unused-disable-directives src",
        "check:import-sort": "npx import-sort --list-different 'src/**/*.js'",
        "check:prettier": "npx prettier --list-different 'src/**/*.js'",
        "clean": "rimraf dist",
        "fix": "npm run fix:prettier && npm run fix:import-sort",
        "fix:import-sort": "npx import-sort --write 'src/**/*.js'",
        "fix:prettier": "npx prettier --write 'src/**/*.js'",
        "prepublish": "in-publish && npm run actually-prepublish || not-in-publish",
        "test": "npm run test:flow && npm run test:typescript && npm run test:jest && npm run check",
        "test:flow": "npx flow check",
        "test:jest": "npx jest --coverage",
        "test:typescript": "dtslint --localTs node_modules/typescript/lib src/types"
    },
    "files": [
        "dist/"
    ],
    "keywords": [
        "itertool",
        "itertools",
        "node-itertools"
    ],
    "devDependencies": {
        "@babel/cli": "^7.15.7",
        "@babel/core": "^7.15.5",
        "@babel/plugin-transform-runtime": "^7.15.0",
        "@babel/preset-env": "^7.15.6",
        "@babel/preset-flow": "^7.14.5",
        "babel-eslint": "^10.1.0",
        "coveralls": "^3.1.1",
        "dtslint": "^4.1.6",
        "eslint": "^7.32.0",
        "eslint-plugin-flowtype": "^6.1.0",
        "flow-bin": "^0.161.0",
        "flow-copy-source": "^2.0.9",
        "flow-typed": "^3.3.1",
        "import-sort-cli": "^6.0.0",
        "import-sort-parser-babylon": "^6.0.0",
        "import-sort-style-eslint": "^6.0.0",
        "import-sort-style-module": "^6.0.0",
        "in-publish": "^2.0.1",
        "jest-cli": "^27.2.4",
        "prettier": "^2.8.7",
        "rimraf": "^3.0.2",
        "tsup": "^6.7.0",
        "typescript": "^4.1.6"
    },
    "types": "./dist/types/index.d.ts",
    "githubUrl": "https://github.com/nvie/itertools.js",
    "sideEffects": false
}
